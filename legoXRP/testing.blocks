from XRPLib.board import Board
from XRPLib.imu import IMU
import time

board = Board.get_default_board()

imu = IMU.get_default_imu()
imu.calibrate(1)


while not (board.is_button_pressed()):
  print(imu.get_acc_x())
  time.sleep(0.1)



